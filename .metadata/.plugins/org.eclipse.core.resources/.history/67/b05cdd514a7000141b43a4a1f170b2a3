
public class recursion {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Count(10);
		CountSum (10);
	}
		public static void Count(int n){
			if (n>1){
				Count(n-1);
		}
		
		System.out.println(n);
	}


// print numbers 1-10

	public static int CountSum (int max){
		if (max>1){
			return max +(max-1);
		}
		System.out.println(max);
		return 1;
	}
	}
	