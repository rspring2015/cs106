
public class recursion {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Count(10);
		Sum (10);
	}
		public static void Count(int n){
			if (n>1){
				Count(n-1);
		}
		
		System.out.println(n);
	}


// print numbers 1-10

	public static void Sum (int n){
		if (n>1){
			Sum(n);
		}
		System.out.println(n);
	}
	}
	